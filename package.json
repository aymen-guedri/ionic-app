{
  "name": "smart-parking",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "backend": "concurrently \"npm run backend:imagekit\" \"npm run backend:firebase\"",
    "backend:imagekit": "node imagekit-server.cjs",
    "backend:firebase": "firebase emulators:start --only functions,firestore",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:full": "concurrently \"npm run backend:imagekit\" \"npm run dev\" --names \"ImageKit,Frontend\" --prefix-colors \"blue,green\"",
    "setup:admin": "node setup-admin.mjs",
    "setup:simple": "node setup-simple.mjs",
    "create:admin": "node create-admin.mjs",
    "setup:db": "npm run setup:admin"
  },
  "dependencies": {
    "@capacitor/app": "8.0.0",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/core": "8.0.1",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/status-bar": "8.0.0",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "imagekit": "^6.0.0",
    "ionicons": "^7.4.0",
    "nodemon": "^3.1.11",
    "qrcode": "^1.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.1",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.24.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "description": "An Ionic project"
}
